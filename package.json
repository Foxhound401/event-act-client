{
  "name": "parcel-reactjs",
  "version": "1.0.0",
  "description": "A minimal React.js boilerplate with parcel as bundler",
  "main": "index.js",
  "scripts": {
    "dev:client": "parcel watch ./src/index.html --out-dir dist",
    "build:client": "rm -rf dist && parcel build ./src/index.html --no-source-maps --out-dir dist --public-url ./",
    "prebuild": "rm -rf dist",
    "build:start": "npm run build && npm start",
    "start": "parcel ./src/index.html",
    "deploy": "npm run build:client && firebase deploy --only hosting:primer-client"
  },
  "keywords": [
    "parcel",
    "react"
  ],
  "author": "“Robin <“wrobin@gmx.net”> (https://www.robinwieruch.de)",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.21.2",
    "dotenv": "^8.1.0",
    "firebase": "^7.0.0",
    "query-string": "^6.9.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-player": "^1.14.2",
    "react-router-dom": "^5.1.2",
    "react-spring": "^8.0.27",
    "react-use-gesture": "^6.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-watch": "^5.1.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.18.2"
  }
}
